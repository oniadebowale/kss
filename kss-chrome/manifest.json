{
  "name": "kss",
  "version": "0.1.3",
  "description": "browser security via keystroke biometrics",
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "exclude_matches": ["http://mail.google.com/*",
                          "https://mail.google.com/*",
                          "http://yuze.no-ip.org/kss/options.html",
                          "http://yuze.no-ip.org/kss/oid_callback.html*"],
      "js": [ 
              "classy.js",
              "jquery-1.6.2.js",
              "keylogger.js",
              "data.js",
              "kss.js",
              "main.js"
            ],
      "all_frames": false
    },
    {
      "matches": ["http://mail.google.com/*",
                  "https://mail.google.com/*"],
      "js": [
              "classy.js",
              "jquery-1.6.2.js",
              "keylogger.js",
              "data.js",
              "kss.js",
              "main-gmail.js"
            ],
      "all_frames": false
    },
    {
      "matches": ["http://yuze.no-ip.org/kss/options.html"],
      "css": [ "google-prettyprint/prettify.css" ],
      "js": [
              "jquery-1.6.2.js",
              "google-prettyprint/prettify.js",
              "options.js"
            ],
      "all_frames": false
    },
    {
      "matches": ["http://yuze.no-ip.org/kss/oid_callback.html*"],
      "js": [
              "jquery-1.6.2.js",
              "oid_callback.js"
            ],
      "all_frames": false
    }
  ],
  "permissions": [
    "http://*/*", "https://*/*"
  ],
  "update_url": "http://yuze.no-ip.org/kss/extension/kss-updates.xml",
  "options_page": "options.html"
}
